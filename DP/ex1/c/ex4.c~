#include <stdio.h>

main(){
	unsigned char a[]={0xaa,0xaa,0xaa,0xaa};
	unsigned char b[]={0xbb,0xbb,0xbb,0xbb};
	int i;
	/**
	 * Compile and run. Explain the output of this program. 
         * Make use of grep:
         *   grep will filter output and only show lines that contain 'aa'
         * ./ex4 | grep aa
	 **/
	for (i=0;i<10000;i++){
		printf("b[%d]=%x\n",i,b[i]);
	}

}



